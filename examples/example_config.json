{
    "name": "Example site",
    "url": "https://examplesite.org/",
    "output": "./output.csv",
    "category_regex": "\\/category-\\w+.*",
    "product_regex": "\\/product[\\w\\d-_]+.*",
    "product": {
        "name": "h1",
        "sku": ".product-sku/attribute-value",
        "price": ".price",
        "old_price": ".old-price",
        "image": ".product-image img/src",
        "description": ".product-description",
        "category": ".breadcrumbs a:nth-last-child(2)",
        "brand": ".breadcrumbs a:nth-last-child(3)",
        "stock": ".stock",
        "ean": ".product-ean/value"
    }
}
